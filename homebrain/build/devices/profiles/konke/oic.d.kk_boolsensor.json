{
    "devicetype": "oic.d.kk_boolsensor",
    "supertype": "oic.d.boolsensor",
    "devicename": "控客人体感应",
    "manufacture": "Konke",
    "version": "0.9.2",
    "profile": {
        "OnlineState": {
            "tag": "在线状态",
            "write": "F",
            "read": "T",
            "type": "enum",
            "range": {
                "0": "离线",
                "1": "在线"
            }
        },
        "normal": {
            "tag": "触发告警",
            "read": "T",
            "write": "F",
            "type": "boolean",
            "range": {
                "0": "正常",
                "1": "有人"
            },
            "unit": ""
        },
        "dismantle": {
            "tag": "防拆告警",
            "read": "T",
            "write": "F",
            "type": "boolean",
            "range": {
                "0": "正常",
                "1": "拆除"
            },
            "unit": ""
        },
        "low_power": {
            "tag": "低电量报警",
            "read": "T",
            "write": "F",
            "type": "boolean",
            "range": {
                "0": "正常",
                "1": "电量低"
            },
            "unit": ""
        },
        "battery": {
            "tag": "电量",
            "read": "T",
            "write": "F",
            "type": "int",
            "range": "v >= 0 and v <= 100",
            "unit": ""
        }
    }
}
